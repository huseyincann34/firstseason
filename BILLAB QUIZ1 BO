#include <stdio.h>
#include <stdlib.h>
/*Kullanıcıdan x ve y değerlerini isteyiniz
Büyük olan değerin küçük olan değere modunu alınız.
--if yapısı kullanılması zorunludur.*/

  int main () {
  int x,y;
  printf("x degerini giriniz: ");
  scanf("%d",&x);
  printf("y degerini giriniz: ");
  scanf("%d",&y);
  if(x>y) {
    printf("%d mod %d: %d",x,y,x%y);
  }
  else {
    printf("%d mod %d: %d",y,x,y%x);
  }
  return 0;
  }
